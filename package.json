    },
    "devDependencies": {
        "chrome-aws-lambda": "^10.1.0",
        "puppeteer-core": "^10.4.0"
    },
    "dependencies": {
        "sketch-gallery": "github:CostardRouge/sketch-gallery#0.0.8"
    }
}
